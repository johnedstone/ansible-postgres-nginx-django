""" https://gunicorn.org/

gunicorn --bind 0.0.0.0:8080 hello_world:app
"""
def app(environ, start_response):
    data = b"{{ hello_world_msg | default("Hello, World!") }}\n"
    start_response("200 OK", [
        ("Content-Type", "text/plain"),
        ("Content-Length", str(len(data)))
    ])
    return iter([data])

